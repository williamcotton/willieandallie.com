{
  "name": "willieandallie.com",
  "version": "1.0.0",
  "description": "Willie & Allie",
  "main": "src/electron/main.js",
  "engines": {
    "node": "6.x.x",
    "npm": "3.x.x"
  },
  "scripts": {
    "test": "npm run build && tapes test/**/*.js test/**/**/*-spec.js | tap-spec",
    "build": "make",
    "watch": "nodemon -q -x 'npm test'",
    "build-dev": "watchify src/browser/index.js -o public/build.js -t babelify -v -d",
    "build-css-dev": "nodemon -w ./src --ext css,scss --exec make -- build_debug_css",
    "start-dev": "NODE_ENV=development foreman start -f Procfile_dev",
    "db:migrate:currentVersion": "knex -- migrate:currentVersion",
    "db:migrate:latest": "knex -- migrate:latest",
    "db:migrate:make": "knex -- migrate:make",
    "db:migrate:rollback": "knex -- migrate:rollback",
    "lint": "eslint ./src ./test ./migrations",
    "postinstall": "make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamcotton/willieandallie.com.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/williamcotton/willieandallie.com/issues"
  },
  "browser": {
    "request": "browser-request"
  },
  "homepage": "https://github.com/williamcotton/willieandallie.com",
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.0",
    "browser-express": "^2.6.0",
    "browser-request": "^0.3.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.9.0",
    "debug": "^2.2.0",
    "ejs": "^2.4.2",
    "express": "^4.14.0",
    "graphql": "^0.6.0",
    "inflection": "^1.10.0",
    "jsonwebtoken": "^7.0.1",
    "knex": "^0.11.7",
    "moment": "^2.13.0",
    "node-jsx": "^0.13.3",
    "node-uuid": "^1.4.7",
    "on-headers": "^1.0.1",
    "pg": "^6.0.1",
    "query-string": "^4.2.2",
    "react": "^15.2.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.2.0",
    "react-faux-dom": "^2.6.2",
    "request": "^2.72.0",
    "sendgrid": "^3.0.5",
    "uglify-js": "^2.6.4",
    "validator": "^5.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "cheerio": "^0.20.0",
    "electron-reload": "^1.0.0",
    "eslint": "^3.0.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "jsdom": "^9.3.0",
    "localStorage": "^1.0.3",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "react-addons-test-utils": "^15.2.0",
    "router": "^1.1.4",
    "tap-spec": "^4.1.1",
    "tapes": "^4.0.0",
    "watchify": "^3.7.0"
  }
}
